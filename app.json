{
  "expo": {
    "name": "TUTility",
    "slug": "TUTility",
    "version": "3.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tutility",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "me.rinrin.tutility",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "me.rinrin.tutility"
    },
    "locales": {
      "ja": "./languages/japanese.json"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "997efd75-4b04-4be7-a856-cd8e4aff6530"
      }
    }
  }
}
